{
  "manifest_version": 2,

  "name": "Auto Youtube Ad Mute",
  "description": "This ad mutes youtube ad audio",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["libs/jquery-3.1.0.min.js", "contentScript.js"],
      "all_frames" : true,
      "run_at": "document_end"
    }
  ], 
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Auto Mute Options"
  },
  "background": {
    "scripts": ["backgroundScript.js"]
  },
  "permissions": [
    "tabs", "*://www.youtube.com/*"
  ]
}